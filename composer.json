{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-rdkafka": "*",
        "fideloper/proxy": "^4.4.1",
        "friendsofphp/php-cs-fixer": "^v3.4.0",
        "fruitcake/laravel-cors": "^v2.0.4",
        "guzzlehttp/guzzle": "^7.4.0",
        "illuminate/redis": "^v8.74.0",
        "laravel-lang/lang": "^10.1.10",
        "laravel/framework": "^v8.68.1",
        "laravel/tinker": "^v2.6.2",
        "mateusjunges/laravel-kafka": "^v1.1.3",
        "spatie/data-transfer-object": "^3.7.2",
        "stechstudio/laravel-php-cs-fixer": "^v2.3.0",
        "zircote/swagger-php": "^3.3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.16.0",
        "fakerphp/faker": "^v1.16.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "laravel/sail": "^v1.12.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^v5.10.0",
        "nunomaduro/larastan": "^1.0.1",
        "phpstan/phpstan": "^1.2.0",
        "phpunit/phpunit": "^9.5.10",
        "predis/predis": "^v1.1.9",
        "squizlabs/php_codesniffer": "^3.6.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix ./ --config=./.php_cs",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
